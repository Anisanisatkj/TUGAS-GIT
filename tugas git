def run(self):
	while not self.game_over:
		self.surface.blit(self.background_image, (0, 0))

		self.handle_events()
		self.update()
self.draw()



		pygame.display.update()
		self.clock.tick(self.frame_rate)
def on_quit(button):
    self.game_over = True
	self.is_game_running = False

